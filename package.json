{
  "name": "@fieldfare/core",
  "version": "1.0.0",
  "description": "Backend framework for distributed networks",
  "main": "./src/platforms/shared/CommonExports.js",
  "type": "module",
  "bin": {
    "fieldfare": "./bin/fieldfare.js",
    "ffsetup": "./bin/ffsetup",
    "ffenvconfig": "./bin/ffenvconfig",
    "ffhost": "./bin/ffhost"
  },
  "scripts": {
    "setup": "node bin/ffsetup.js",
    "host": "node bin/ffhost.js",
    "envconfig": "node bin/ffenvconfig.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --verbose --runInBand",
    "coverage": "jest --collectCoverage --",
    "debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moduhub/fieldfare.git"
  },
  "keywords": [
    "backend, framework, distributed, hash tables, version-control, distributed hash tables, dht, p2p, peer-to-peer, peer to peer, peer, to, peer, p2p, peer2peer,"
  ],
  "author": "Adan Kvitschal",
  "license": "ISC",
  "dependencies": {
    "arg": "^5.0.1",
    "blessed": "^0.1.81",
    "esm": "^3.2.25",
    "inquirer": "^9.1.5",
    "inquirer-file-tree-selection-prompt": "^2.0.5",
    "level": "^8.0.0",
    "uuid": "^9.0.0",
    "websocket": "^1.0.34"
  },
  "exports": {
    ".": {
      "require": "./src/platforms/shared/CommonExports.js",
      "import": "./src/platforms/shared/CommonExports"
    },
    "./browser": {
      "require": "./src/platforms/browser/BrowserExports.js",
      "import": "./src/platforms/browser/BrowserExports"
    },
    "./node": {
      "require": "./src/platforms/node/NodeExports.js",
      "import": "./src/platforms/node/NodeExports"
    },
    "./test": {
      "require": "./src/platforms/test/TestExports.js",
      "import": "./src/platforms/test/TestExports"
    },
    "./services/": {
      "require": "./src/env/services/",
      "import": "./src/env/services/"
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.5.0"
  },
  "bugs": {
    "url": "https://github.com/moduhub/fieldfare/issues"
  },
  "homepage": "https://github.com/moduhub/fieldfare#readme",
  "directories": {
    "doc": "doc",
    "test": "test"
  }
}
